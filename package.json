{
  "name": "SilverBullet2",
  "version": "0.4.0",
  "description": "rest api boilerplate with typescript",
  "keywords": [
    "express",
    "sql",
    "knex",
    "typescript",
    "objection",
    "rest",
    "api",
    "boilerplate"
  ],
  "main": "dist/server",
  "scripts": {
    "postinstall": "node init.js",
    "dev": "tsc ; node dist/checkServer.js && node ./node_modules/tsc-watch/tsc-watch.js --onSuccess 'node dist/server.js'",
    "build": "tsc",
    "clean": "rm -rf dist",
    "start": "node dist/server",
    "init": "knex --knexfile ./src/knexfile.ts migrate:latest ",
    "lint": "tslint 'src/**/*.ts?(x)' -t verbose",
    "test": "mocha test/**/**.spec.js",
    "dockerInit": "docker exec sb2_node_1 npm run init"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "@types/connect-redis": "0.0.6",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "connect-redis": "^3.3.0",
    "cors": "2.7.1",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "helmet": "3.13.0",
    "http": "0.0.0",
    "knex": "2.4.0",
    "knex-db-manager": "0.4.0",
    "morgan": "1.9.0",
    "multer": "1.3.0",
    "mysql": "2.14.1",
    "objection": "0.8.8",
    "ramda": "0.24.1",
    "tslint-config-airbnb": "5.3.0"
  },
  "devDependencies": {
    "@types/express": "4.0.37",
    "@types/express-session": "1.15.3",
    "@types/knex": "0.0.61",
    "@types/multer": "1.3.3",
    "@types/node": "8.0.28",
    "chai": "4.1.2",
    "chai-http": "4.2.0",
    "colors": "1.1.2",
    "eslint": "5.5.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.4",
    "ts-node": "3.3.0",
    "tsc-watch": "1.0.8"
  }
}
